{
  "projectType": "library",
  "sourceRoot": "libs/rx/src",
  "prefix": "ngry",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/rx"],
      "options": {
        "project": "libs/rx/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/rx/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/rx/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/rx"],
      "options": {
        "jestConfig": "libs/rx/jest.config.ts",
        "passWithNoTests": true,
        "verbose": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/rx/**/*.ts", "libs/rx/**/*.html"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "master",
        "preset": "angular",
        "tagPrefix": "${projectName}@"
      }
    }
  },
  "tags": []
}
